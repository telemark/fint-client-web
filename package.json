{
  "name": "fint-client-web",
  "version": "1.0.2",
  "description": "Get fint token frontend",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "node server.js",
    "build": "next build",
    "now-build": "next build && next export",
    "start": "NODE_ENV=production node server.js",
    "deploy": "npm test && now && now alias",
    "export": "next build && next export",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/fint-client-web.git"
  },
  "keywords": [],
  "author": {
    "name": "Jonas Maccyber Enge",
    "email": "jonas.enge@gmail.com",
    "url": "https://github.com/maccyber"
  },
  "contributors": [
    {
      "name": "Geir GÃ¥sodden",
      "email": "geir.gasodden@pythonia.no",
      "url": "https://github.com/zrrrzzt"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/telemark/fint-client-web/issues"
  },
  "dependencies": {
    "axios": "0.18.0",
    "check-valid-url": "0.0.2",
    "fint-get-token": "1.0.5",
    "next": "8.0.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-json-view": "1.19.1"
  },
  "devDependencies": {
    "ava": "1.4.1",
    "babel-eslint": "10.0.1",
    "coveralls": "3.0.3",
    "nyc": "13.3.0",
    "standard": "12.0.1"
  },
  "engines": {
    "node": ">=10.15.1"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  }
}
